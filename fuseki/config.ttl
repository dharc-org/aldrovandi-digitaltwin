# Configurazione dataset chad-kg per Fuseki
@prefix :        <#> .
@prefix fuseki:  <http://jena.apache.org/fuseki#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix tdb2:    <http://jena.apache.org/2016/tdb#> .

# Dataset chad-kg - persistente con TDB2
:service_chad_kg rdf:type fuseki:Service ;
    fuseki:name                      "chad-kg" ;
    fuseki:serviceQuery              "query", "sparql" ;
    fuseki:serviceReadGraphStore     "get" ;
    fuseki:serviceReadWriteGraphStore "data" ;
    fuseki:serviceUpdate             "update" ;
    fuseki:serviceUpload             "upload" ;
    fuseki:dataset                   :dataset_chad_kg ;
    .

:dataset_chad_kg rdf:type tdb2:DatasetTDB2 ;
    tdb2:location "/fuseki/databases/chad-kg" ;
    .
